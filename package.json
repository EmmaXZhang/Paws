  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.1.0",
    "concurrently": "^8.2.2",
    "cookie-parse": "^0.4.0",
    "cookie-parser": "^1.4.6",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "react-router-dom": "^6.22.3"
  }
