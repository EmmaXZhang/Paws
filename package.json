{
  "name": "paws",
  "version": "1.0.0",
  "description": "Clone this repo to provide the starter code for a comprehensive MERN-Stack project including token-based authentication.",
  "main": "index.js",
  "scripts": {
    "server": "npm run dev --prefix backend",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
